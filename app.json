{
  "expo": {
    "name": "firebase-notes",
    "slug": "firebase-notes",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/monocromatic-red.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/monocromatic-red.png",
        "backgroundColor": "#FF0A0A"
      },
      "package": "firebase.notes.test"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/monocromatic-red.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/monocromatic-red.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#FF0A0A"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "b1977dec-4848-41e3-a0df-937c3c48a869"
      }
    }
  }
}
